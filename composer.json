{
  "name": "kmalarifi97/gateway-auth",
  "description": "payment gateways library for Laravel 11.",
  "type": "library",
  "require": {
    "php": "^8.2",
    "illuminate/support": "^11.0",
    "illuminate/http": "^11.0"
  },
  "autoload": {
    "psr-4": {
      "kmalarifi97\\GatewayAuth\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "kmalarifi97\\GatewayAuth\\GatewayAuthServiceProvider"
      ]
    }
  },
  "authors": [
    {
      "name": "Khalid Alâ€‘Arifi",
      "email": "k.alarifi@elc.edu.sa"
    }
  ],
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require-dev": {
    "pestphp/pest": "^3.8",
    "orchestra/testbench": "9.13.1"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
